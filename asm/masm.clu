# extend

nul_dev = proc () returns (str)
	return("/dev/null")
	end nul_dev

code_temp = proc () returns (str)
	return("_c_")
	end code_temp

masm_dir = proc () returns (str)
	return(_system_root() || "/asm")
	end masm_dir

bin_dir = proc () returns (str)
	return(_system_root() || "/bin")
	end bin_dir
